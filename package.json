{
  "name": "@knorm/relations",
  "version": "1.3.1",
  "description": "Relations plugin for @knorm/knorm",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "docker-compose run --rm knorm-relations",
    "test:mocha": "mocha --exit --recursive",
    "coverage": "nyc npm run test:mocha",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "postgres": "docker-compose exec postgres echo 'postgres running ...' || docker-compose up -d postgres",
    "postgres:stop": "docker-compose stop postgres",
    "postgres:remove": "docker-compose rm -sf postgres"
  },
  "author": "Joel Mukuthu <joelmukuthu@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "@knorm/knorm": "^1.9.0",
    "@knorm/postgres": "^1.3.0"
  },
  "devDependencies": {
    "@knorm/knorm": "1.11.1",
    "@knorm/postgres": "1.3.4",
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/git": "7.0.8",
    "@semantic-release/github": "5.2.9",
    "@semantic-release/npm": "5.1.3",
    "coveralls": "3.0.2",
    "eslint": "5.12.0",
    "eslint-config-ganintegrity": "3.1.1",
    "knex": "0.15.2",
    "lodash": "4.17.11",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.5",
    "nyc": "13.1.0",
    "pg": "7.8.0",
    "prettier": "1.15.3",
    "proxyquire": "2.1.0",
    "semantic-release": "15.13.3",
    "unexpected": "10.40.0",
    "unexpected-knex": "1.5.0"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "keywords": [
    "knorm",
    "plugin",
    "relations",
    "joins"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/knorm/relations.git"
  },
  "bugs": {
    "url": "https://github.com/knorm/relations/issues"
  },
  "homepage": "https://github.com/knorm/relations#readme",
  "dependencies": {}
}
